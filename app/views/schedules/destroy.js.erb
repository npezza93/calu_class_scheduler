<% if @service.destroyed? %>
  <% @service.modified.each do |offering_id| %>
    $("[data-offering-id=<%= offering_id %>] input")[0].disabled = false;
  <% end %>
  var element = $("[data-offering-id=<%= @service.schedule.offering_id %>] a");
  element.data("method", "post");
  element.attr("href", "<%= schedules_path(offering_id: @service.schedule.offering_id) %>");
  element.find("input")[0].checked = false;
<% end %>
