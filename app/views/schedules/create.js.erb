<% if @service.saved? %>
  <% @service.modified.each do |offering_id| %>
    $("[data-offering-id=<%= offering_id %>] input")[0].disabled = true;
  <% end %>
  var element = $("[data-offering-id=<%= @service.schedule.offering_id %>] a");
  element.data("method", "delete");
  element.attr("href", "<%= schedule_path(@service.schedule.offering_id) %>");
  element.find("input")[0].checked = true;
<% end %>
