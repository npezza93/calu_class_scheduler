var schedule = $("#<%= @schedule.offering_id %>");
schedule.find('input').prop('checked', true);
var link = schedule.find('a');
link.attr('data-method', 'delete');
link.data().method = 'delete';
link.attr('href', "<%= schedule_path(@schedule.offering_id) %>");

<% @hidden_offerings.each do |id| %>
  var schedule = $("#<%= id %>");
  schedule.fadeOut();
<% end %>
